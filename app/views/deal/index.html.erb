<div class="col-lg-8">
<div id="all-number" class="text-center">
<% 0.upto(74) do |i| %>
  <%= form_tag({controller: "deal", action: "add"}, remote: true, method: "get", id: "form-#{i}") do%>
    <%= hidden_field_tag(:number, @all_numbers[i]) %>
    <%= submit_tag(@all_numbers[i], id: "btn-number-#{i}", onclick: "changeStyle(this.id);") %>
  <% end %>
<% end %>
</div><br>
<h5>Please use your smartphone's qr-code-reader to access game</h5>
<div id='div-qr'>
  <%= image_tag @qr %>
</div>
</div>

<div class="col-lg-4">
	<h3 class="text-center"> Events </h3>
	<div id="log_event">
	</div>
</div